{
  "manifestVersion": "1.0",
  "id": "AAG-GqJ8WhI",
  "name": "Shopinista App",
  "description": "Integraci√≥n de backend con OAuth y API para ShopinistaMeta",
  "version": "1.0.0",
  "permissions": ["api", "oauth"],
  "backend": {
    "url": "https://shopinista-app.vercel.app"
  },
  "intents": [
    {
      "name": "open",
      "entryPoint": "dist/app.js"
    }
  ],
  "oauth": {
    "authorizeUrl": "https://shopinista-app.vercel.app/oauth/authorize",
    "tokenUrl": "https://shopinista-app.vercel.app/oauth/token",
    "revokeUrl": "https://shopinista-app.vercel.app/oauth/revoke"
  },
  "api": {
    "baseUrl": "https://shopinista-app.vercel.app/api"
  }
}
