{
  "private": true,
  "name": "new_app_canva",
  "version": "1.0.0",
  "description": "An example Canva Data Connector App",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Canva Pty Ltd.",
  "scripts": {
    "extract": "formatjs extract \"src/**/*.{ts,tsx}\" --out-file dist/messages_en.json",
    "build:client": "webpack --config webpack.config.ts --mode production && npm run extract",
    "build:server": "tsc -p server/tsconfig.server.json",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "webpack serve --config webpack.config.dev.ts --mode development --port 8080",
    "dev:server": "cd server && ts-node-dev --respawn --transpile-only src/server.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "format": "prettier '**/*.{css,ts,tsx,js,json,md}' --no-config --write",
    "format:check": "prettier '**/*.{css,ts,tsx,js,json,md}' --no-config --check",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "lint:types": "tsc --noEmit",
    "start": "node server/dist/server.js",
    "start:preview": "npm run start -- --preview",
    "test": "jest --no-cache",
    "test:watch": "jest --watchAll",
    "test:update": "npm run test -- -u",
    "postinstall": "ts-node --loader ts-node/esm --transpile-only scripts/copy_env.ts"
  },
  "dependencies": {
    "@canva/app-hooks": "^0.0.0-beta.4",
    "@canva/app-i18n-kit": "^1.2.0",
    "@canva/app-middleware": "^0.0.0-beta.4",
    "@canva/app-ui-kit": "^5.5.0",
